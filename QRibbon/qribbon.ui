/**********微调器**********/
QSpinBox,
QDoubleSpinBox {
  border-radius: 4px;
  min-height: 24px;
  min-width: 40px;
  border: 1px solid rgb(43,87,154);
  background: white;
}

QSpinBox:enabled,
QDoubleSpinBox:enabled {
  color: rgb(60, 60, 60);
}

QSpinBox:enabled:hover,
QSpinBox:enabled:focus {
  color: rgb(51, 51, 51);
}

QSpinBox:!enabled,
QDoubleSpinBox:!enabled {
  color: rgb(210, 210, 210);
  background: transparent;
}

QSpinBox::up-button,
QDoubleSpinBox::up-button {
  subcontrol-origin: border;
  subcontrol-position: top right; /* position at the top right corner */
  /* border-left: 1px solid rgb(255,255,255); */
  width: 18px;
  height: 12px;
  border-top-right-radius: 4px;
  image: url(:/White/topArrow);
}

QSpinBox::up-button:!enabled {
  background: transparent;
}

QSpinBox::up-button:enabled:hover {
  background: rgb(255, 255, 255, 30);
}

QSpinBox::down-button,
QDoubleSpinBox::down-button {
  /* border-left: 1px solid rgb(255,255,255); */
  width: 18px;
  height: 12px;
  border-bottom-right-radius: 4px;
  image: url(:/White/bottomArrow);
  subcontrol-origin: border;
  subcontrol-position: bottom right; /* position at bottom right corner */
}

QSpinBox::down-button:!enabled {

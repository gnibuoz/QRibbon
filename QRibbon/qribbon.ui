QPushButton,QPushButton#pushButtonStyle,QPushButton#pushButtonMinimizeTab,QPushButton#pushButtonMaxTab,QPushButton#pushButtonClose,QPushButton#pushButtonFullScreen,QPushButton#pushButtonMinimum{
	background:transparent;
	font-weight:bold;
	margin:0px;
	border:0px solid #fff;
	outline:none;
}

QPushButton:hover,QPushButton#pushButtonFullScreen:hover,QPushButton#pushButtonMinimum:hover,QPushButton#pushButtonMaxTab:hover{
	background:rgba(0,0,0,40);
}

QPushButton:pressed,QPushButton#pushButtonFullScreen:pressed,QPushButton#pushButtonMinimum:pressed,QPushButton#pushButtonMaxTab:pressed{
	background:rgba(0,0,0,80);
	padding-top:3px;
}

QPushButton#pushButtonClose:hover{
	background:rgba(232,17,35,255);
}

QPushButton#pushButtonClose:pressed{
	background:rgba(232,17,35,200);
}

QPushButton#pushButtonMinimizeTab,QPushButton#pushButtonMaxTab{
	color: rgb(112, 112, 112);
}

QPushButton#pushButtonMinimizeTab:hover,QPushButton#pushButtonMaxTab:hover{
	background:rgb(177,177,177);
}


